{
  "name": "ahwr-public-user-ui",
  "version": "0.0.2",
  "description": "Public user UI for AHWR service",
  "homepage": "https://github.com/DEFRA/ahwr-public-user-ui",
  "main": "app/index.js",
  "type": "module",
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "pretest": "npm run lint",
    "format": "prettier --write \"**/*.{cjs,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{cjs,js,json,md}\"",
    "test": "jest --runInBand",
    "test:watch": "jest --coverage=false --onlyChanged --watch --runInBand",
    "test:watch:all": "npm-run-all --parallel test:watch build:watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test:debug": "node --inspect-brk=0.0.0.0 ./node_modules/jest/bin/jest.js --coverage=false --onlyChanged --watch --runInBand --no-cache",
    "start:watch": "npm-run-all --parallel build:watch start:nodemon",
    "start:debug": "nodemon -r dotenv/config --inspect-brk=0.0.0.0 --ext css,js,njk --legacy-watch app/index.js",
    "start:nodemon": "nodemon -r dotenv/config --inspect=0.0.0.0 --ext css,js,njk --legacy-watch app/index.js",
    "jest": "jest",
    "postinstall": "patch-package"
  },
  "author": "Defra",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@2toad/profanity": "3.2.0",
    "@aws-sdk/client-s3": "3.982.0",
    "@aws-sdk/s3-request-presigner": "3.982.0",
    "@defra/hapi-tracing": "1.30.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/boom": "9.1.4",
