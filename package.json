    "@hapi/crumb": "8.0.1",
    "@hapi/hapi": "21.4.3",
    "@hapi/inert": "6.0.5",
    "@hapi/vision": "6.1.0",
    "@hapi/wreck": "18.1.0",
    "@hapi/yar": "10.1.1",
    "@ministryofjustice/frontend": "5.2.1",
    "aws-embedded-metrics": "4.2.0",
    "ffc-ahwr-common-library": "3.5.4",
    "form-data": "4.0.4",
    "global-agent": "3.0.0",
    "govuk-frontend": "5.12.0",
